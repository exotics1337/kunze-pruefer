<UserControl x:Class="kunze_prüfer.Views.Auftragsverwaltung.DBSichten.AuftragErledigt"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:kunze_prüfer.Views.Auftragsverwaltung.DBSichten"
             mc:Ignorable="d"
             d:DesignHeight="500" d:DesignWidth="680">
    <Grid>
        <ScrollViewer>
            <StackPanel>
                <GroupBox Height="100" Margin="15,15,15,15" VerticalAlignment="Top" Header="Auftrag erledigt">
                    <StackPanel >
                        <TextBlock Text="Auftrag ist Erledigt!" FontSize="18" Margin="5" FontWeight="Light" />
                    </StackPanel>
                </GroupBox>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
